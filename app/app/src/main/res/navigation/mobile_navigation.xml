<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_category">

    <fragment
        android:id="@+id/navigation_category"
        android:name="ru.nnov.app.ui.category.CategoryFragment"
        android:label="@string/title_catalog"
        tools:layout="@layout/fragment_category" />

    <fragment
        android:id="@+id/navigation_bucket"
        android:name="ru.nnov.app.ui.bucket.BucketFragment"
        android:label="@string/title_bucket"
        tools:layout="@layout/fragment_bucket" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="ru.nnov.app.ui.profile.ProfileFragment"
        android:label="@string/title_prifile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/navigation_login"
        android:name="ru.nnov.app.ui.login.LoginFragment"
        android:label="@string/title_prifile"
        tools:layout="@layout/fragment_login" />
</navigation>